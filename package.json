{
  "name": "goharvey.com",
  "version": "0.1.0",
  "description": "Runs goharvey.com",
  "engines": {
    "node": "7.10.0",
    "npm": "5.4.2"
  },
  "private": true,
  "scripts": {
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch-poll": "npm run watch -- --watch-poll",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production",
    "lint": "./node_modules/eslint/bin/eslint.js -c .eslintrc --ignore-path .eslintignore resources/assets/js --ext .vue --ext .js --fix",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "sassdoc": "./node_modules/.bin/sassdoc ./resources/assets/scss --config ./sassdoc-config.json",
    "test": "NODE_ENV=test karma start karma.conf.js",
    "test:watch": "npm test -- --watch",
    "validate": "npm ls"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "dependencies": {
    "ajv": "^5.4.0",
    "animate.css": "^3.5.2",
    "axios": "^0.15.3",
    "babel-polyfill": "^6.26.0",
    "bootstrap-sass": "^3.3.7",
    "bulma": "^0.3.0",
    "card": "^2.4.0",
    "cross-env": "^3.2.3",
    "es6-promise": "^4.2.2",
    "get-env": "^0.5.10",
    "highlight.js": "^9.12.0",
    "laravel-mix": "^1.7.2",
    "lodash": "^4.17.4",
    "moment": "^2.20.1",
    "moment-timezone": "^0.5.14",
    "object-diff": "0.0.4",
    "pusher-js": "^4.2.1",
    "q": "^1.5.1",
    "quill": "^1.3.4",
    "sass-loader": "^6.0.3",
    "stripe": "^4.23.2",
    "style-loader": "^0.16.1",
    "vee-validate": "^2.0.0-rc.27",
    "vue": "^2.5.13",
    "vue-quill-editor": "^3.0.1",
    "vue-resource": "^1.3.5",
    "vue-router": "^2.8.1",
    "vue-slider-component": "^2.4.7",
    "vue-spinner": "^1.0.3",
    "vue-the-mask": "^0.11.1",
    "vuedraggable": "^2.15.0",
    "webpack": "^3.10.0",
    "webpack-spritesmith": "^0.3.6"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.5",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "cheerio": "^1.0.0-rc.2",
    "css-loader": "^0.28.9",
    "editorconfig": "^0.15.0",
    "eslint": "^3.19.0",
    "eslint-plugin-vue": "^3.13.1",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-sinon-chai": "^1.3.3",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.31",
    "karma-webpack": "^2.0.9",
    "mocha": "^3.5.3",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.0.10",
    "precommit-hook": "^3.0.0",
    "sassdoc": "^2.5.0",
    "sassdoc-theme-flippant": "^0.1.0",
    "sinon": "^2.4.1",
    "sinon-chai": "^2.14.0",
    "vue-eslint-parser": "^1.0.0",
    "vue-loader": "^13.7.0",
    "vue-template-compiler": "^2.5.13"
  }
}

    "karma-jsdom-launcher": "^6.1.2",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sinon-chai": "^1.3.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.4",
    "mocha": "^3.5.3",
    "sassdoc": "^2.3.0",
    "sassdoc-theme-flippant": "^0.1.0",
    "sinon": "^4.0.0",
    "sinon-chai": "^2.14.0"
  }

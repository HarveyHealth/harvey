{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1",
        "laravel/framework": "5.4.*",
        "predis/predis": "^1.1",
        "aws/aws-sdk-php": "^3.20",
        "guzzlehttp/guzzle": "^6.2",
        "stripe/stripe-php": "^4.3",
        "lob/lob-php": "^1.7",
        "michelf/php-smartypants": "^1.8",
        "league/commonmark": "^0.15.3",
        "bugsnag/bugsnag-laravel": "^2.0",
        "fzaninotto/faker": "~1.4",
        "barryvdh/laravel-debugbar": "^2.3",
        "laravel/tinker": "^1.0",
        "jsvrcek/ics": "^0.5.0",
        "league/flysystem-aws-s3-v3": "~1.0",
        "fideloper/proxy": "^3.2",
        "doctrine/dbal": "^2.5",
        "propaganistas/laravel-phone": "^2.8",
        "laravel/dusk": "^1.0",
        "spatie/laravel-fractal": "^4.0",
        "laravel/passport": "^3.0",
        "crell/api-problem": "^2.0",
        "wildbit/postmark-php": "^2.3",
        "laravel/homestead": "^5.2",
        "laravel/scout": "^3.0",
        "pusher/pusher-php-server": "^2.6",
        "twilio/sdk": "^5.11",
        "google/apiclient": "^2.2",
        "cviebrock/eloquent-sluggable": "^4.2",
        "shippo/shippo-php": "1.*",
        "laravel/socialite": "^3.0",
        "algolia/algoliasearch-client-php": "^1.23"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "phpunit/php-code-coverage": "~4.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*"
    },
    "autoload": {
        "classmap": [
            "database",
            "august"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize",
            "php artisan migrate --force",
            "php artisan scout:import 'App\\Models\\User'",
            "php artisan scout:import 'App\\Models\\Message'",
            "php artisan scout:import 'App\\Models\\Patient'",
            "php artisan passport:getkeys",
            "php artisan calendar:get_credentials",
            "php artisan cache:forget render_css_app_public",
            "npm run prod"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize",
            "php artisan migrate --force",
            "php artisan scout:import 'App\\Models\\User'",
            "php artisan scout:import 'App\\Models\\Message'",
            "php artisan scout:import 'App\\Models\\Patient'",
            "php artisan view:clear",
            "php artisan cache:clear",
            "php artisan calendar:get_credentials",
            "php artisan cache:clear",
            "npm run dev"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}

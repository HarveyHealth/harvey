{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "algolia/algoliasearch-client-php": "^1.23",
        "aws/aws-sdk-php": "^3.20",
        "barryvdh/laravel-debugbar": "^3.1",
        "bugsnag/bugsnag-laravel": "^2.0",
        "crell/api-problem": "^2.0",
        "cviebrock/eloquent-sluggable": "^4.2",
        "doctrine/dbal": "^2.6",
        "fideloper/proxy": "~3.3",
        "google/apiclient": "^2.2",
        "guzzlehttp/guzzle": "^6.2",
        "jsvrcek/ics": "^0.5.0",
        "laravel/framework": "5.5.*",
        "laravel/passport": "^3.0",
        "laravel/scout": "^3.0",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "^1.0",
        "league/commonmark": "^0.15.3",
        "league/flysystem-aws-s3-v3": "~1.0",
        "lob/lob-php": "^1.7",
        "michelf/php-smartypants": "^1.8",
        "mouf/nodejs-installer": "~1.0",
        "php": "~7.2",
        "predis/predis": "^1.1",
        "propaganistas/laravel-phone": "^2.8",
        "pusher/pusher-php-server": "~3.0",
        "shippo/shippo-php": "1.*",
        "spatie/laravel-fractal": "^5.0",
        "stripe/stripe-php": "^4.3",
        "twilio/sdk": "^5.11",
        "wildbit/postmark-php": "^2.3"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0",
        "phpunit/php-code-coverage": "5.*",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "symfony/thanks": "^1.0",
        "laravel/dusk": "^2.0",
        "laravel/homestead": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories",
            "august"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/dusk"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-install-cmd": [
            "php artisan migrate --force",
            "php artisan scout:import 'App\\Models\\User'",
            "php artisan scout:import 'App\\Models\\Message'",
            "php artisan scout:import 'App\\Models\\Attachment'",
            "php artisan scout:import 'App\\Models\\LabTestResult'",
            "php artisan scout:import 'App\\Models\\Prescription'",
            "php artisan scout:import 'App\\Models\\SoapNote'",
            "php artisan scout:import 'App\\Models\\Patient'",
            "npm install",
            "npm run prod"
        ],

        "post-update-cmd": [
            "php artisan migrate --force",
            "php artisan scout:import 'App\\Models\\User'",
            "php artisan scout:import 'App\\Models\\Message'",
            "php artisan scout:import 'App\\Models\\Attachment'",
            "php artisan scout:import 'App\\Models\\LabTestResult'",
            "php artisan scout:import 'App\\Models\\Prescription'",
            "php artisan scout:import 'App\\Models\\SoapNote'",
            "php artisan scout:import 'App\\Models\\Patient'",
            "php artisan view:clear",
            "php artisan cache:clear",
            "npm install",
            "npm run dev"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist"
    }
}
